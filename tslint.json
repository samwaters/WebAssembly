{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces", 2],
    "interface-name": false,
    "object-literal-shorthand": false,
    "quotemark": [true, "single"],
    "no-empty-interface": false,
    "semicolon": false,
    "whitespace": false
  },
  "rulesDirectory": []
}
